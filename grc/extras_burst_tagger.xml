<?xml version="1.0"?>
<!--
###################################################
## Blocks burst tagger
###################################################
 -->
<block>
    <name>Ex: BurstTagger</name>
    <key>extras_burst_tagger</key>
    <import>import grextras</import>
    <make>grextras.BurstTagger($sps)</make>
    <param>
        <name>Samples per symbol</name>
        <key>sps</key>
        <value>1</value>
        <type>int</type>
    </param>
    <sink>
        <name>in</name>
        <type>complex</type>
    </sink>
    <source>
        <name>out</name>
        <type>complex</type>
    </source>
</block>
